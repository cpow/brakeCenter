<fieldset id="" class="">
	<legend>Send Feedback</legend>	

<% form_for :mail, :url => {:action => "sendMail"} do |f|%>

<%= f.text_field :first %>
<%= f.label "First Name" %><br />

<%= f.text_field :last %>
<%= f.label "Last Name" %><br />

<%= f.text_field :addy %>
<%= f.label "Email" %><br /><br />

<%= f.label "Message" %><br />
<%= f.text_area :body %><br />

<%= f.submit "Send" %>
<% end %>

</fieldset>


